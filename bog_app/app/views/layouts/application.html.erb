<!DOCTYPE html>
<html>
  <head>
    <title>BogApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar navbar-default">
      <ul class="nav nav-tabs">
        <li role="presentation"><%= link_to "All Creatures", creatures_path %></li>
        <li role="presentation"><%= link_to "+ New Creature", new_creature_path %></li>
      </ul>
    </nav>
  	<div class="container">
  		<div class="row">
  			<div class="col-md-6 col-md-offset-3">
			    <%= yield %>
			</div>
		</div>
  	</div>



  </body>
</html>
