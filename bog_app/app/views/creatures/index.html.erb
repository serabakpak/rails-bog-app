<h1>Creatures</h1>

<% @creatures.each do |creature| %>
	<%= creature.name %>
	<%= creature.description %>
	<%= image_tag creature.avatar.url(:medium) %>
	<%= button_to "Show", creature, method: :get %>
	<%= button_to "Edit", edit_creature_path(creature), method: :get %>
	<%= button_to "Delete", creature, method: :delete %>
<% end %>





